# Режим работы (dev/prod)
MODE=dev

# Уровень логирования (DEBUG/INFO/WARNING/ERROR)
LOG_LEVEL=INFO

# OpenRouter API (обязателен в режиме prod)
OPENROUTER_API_KEY=your_openrouter_key_here

# Telegram Bot Token (обязателен в режиме prod)
BOT_TOKEN=your_telegram_bot_token_here

# Модели LLM
MODEL_FAST=openrouter/fast
MODEL_SMART=openrouter/smart

# Дневной лимит токенов
TOKEN_BUDGET_DAILY=50000
